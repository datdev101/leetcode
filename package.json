{
  "scripts": {
    "solution:path": "find $PWD/src -type d -name \"$ID.*\"",
    "start": "npx ts-node \"$(npm run -s solution:path)\"/solve.ts",
    "start:java": "java -ea \"$(npm run -s solution:path)\"/solve.java",
    "gen": "cp -r template \"src/${NAME}\""
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}